{
    "name": "nrep/filament-theme-switcher",
    "description": "A FilamentPHP plugin that allows users to easily switch and customize application themes",
    "keywords": [
        "nrep",
        "laravel",
        "filament",
        "theme",
        "theme-switcher",
        "filament-plugin"
    ],
    "homepage": "https://github.com/nrep/filament-theme-switcher",
    "support": {
        "issues": "https://github.com/nrep/filament-theme-switcher/issues",
        "source": "https://github.com/nrep/filament-theme-switcher"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Isura",
            "email": "info@isura.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0|^4.0",
        "filament/support": "^3.0|^4.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "nrep/filament-theme-switcher": "main"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9|^8.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.1|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Isura\\FilamentThemeSwitcher\\": "src/",
            "Isura\\FilamentThemeSwitcher\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Isura\\FilamentThemeSwitcher\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Isura\\FilamentThemeSwitcher\\FilamentThemeSwitcherServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nrep/filament-theme-switcher"
        }
    ]
}
